


<h1>マイページ</h1>
 <div class="profile">
  <table class="end_user_profile">
		<thead>
		<tr>
			<th></th><th></th>
		</tr>
	    </thead>

	    <tbody>

			<tr>
				<td>名前</td>
				<td>
					<%= @end_user.kanzi_last_name+@end_user.kanzi_first_name %>
				</td>

				<td>カナ</td>
				<td>
					<%= @end_user.kana_last_name+""+@end_user.kana_first_name %>
				</td>
				<td>郵便番号</td>
				<td>
					<%= @address.postal_code%>
				</td>
				<td>住所</td>
				<td>
					<%= @address.address %>
				</td>
				<td>電話番号</td>
				<td>
					<%= @address.phone_number %>
				</td>
				<td>メールアドレス</td>
				<td>
					<%= @end_user.email %>
				</td>
				<td>パスワード</td>
				<td>
					<%= @end_user.password %>
				</td>
			</tr>
 	    </tbody>
     </table>
 </div>


