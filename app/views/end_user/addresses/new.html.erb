<h1>EndUser::Addresses#new</h1>
<p>Find me in app/views/end_user/addresses/new.html.erb</p>

<% form_for(@address) do |f| %>

<h2>郵便番号</h2>
<%= f.number_field :postal_code %>

<h2>住所</h2>
<%= f.text_field :address %>

<h2>電話番号</h2>
<%= f.number_field :phone_number %>

<br>

<%= f.submit "create address" %>
<% end %>










