<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>


<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>名前</th>
			<th>カナ</th>
		</tr>
	</thead>
	<tbody>
			<% @end_users.each do |end_user| %>
		<tr>
			   <td>
				<%= link_to end_user.id,admin_user_path(end_user.id) %>
			    </td>
				<td>
					<%= end_user.kanzi_last_name+end_user.kanzi_first_name %>
				</td>
				<td>
					<%= end_user.kana_last_name+""+end_user.kana_first_name %>
				</td>
		</tr>
			<% end %>
	</tbody>
</table>

